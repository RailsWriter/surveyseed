<body class ="body">	

 <script type="text/javascript">
			mixpanel.track("Landed on Home Page");
 </script>
 
 
 <div id="header">
	 <div id="container1">
		 <%= image_tag image_path('cooltext1784832111.png') %>
	</div>
 </div>

 <div id="hero">
	 <div id="container2">
		<center>
			<p id="bigfont">Our new website is launching soon. Stay tuned.<p>
		</center>
 	</div>
 </div>

 <div id="contact">
    <div id="container3">
		<center><p>Meanwhile, to connect with us drop us a note.</p>
			<div id="p1"><%= alert %></h2></div><br>
			<%= form_for @lead do |f| %>	
					<%= f.text_field :name, placeholder: "Name", :class => "input" %><br><br>
					<%= f.text_field :email, placeholder: "Email", :class => "input" %><br><br>
					<%= f.text_field :phone, placeholder: "Phone", :class => "input" %><br><br>
					<%= f.text_area :message, placeholder: "Message", :class => "textarea" %><br><br>
					<%= f.submit "Send", :class => "button" %>				
			<% end %>
		</center>
   	</div>
 </div>
 
 <script type="text/javascript">
	 	mixpanel.track_forms('entered name', {
	 		name: <%= @lead.name.to_json %>
	 	});
 </script>
 

 <div id="footer">
    <div id="container4">
		<!-- %= image_tag image_path('images.jpeg') % -->
		<p align="center" id="smallfont">Copyright Â© by Ketsci. All rights reserved.
		<!-- a href="privacy_policy_11032014.pdf">Privacy Policy.</a -->
		<!-- a href="terms_and_conditions_11032014.pdf">Terms and Conditions.</a -->
		</p>
	</div>
 </div>

</body>

